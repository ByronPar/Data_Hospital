@model Practica2.Models.PACIENTE

@{
    ViewBag.Title = "reporte2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h2>reporte2 - Paciente con más citas </h2>

<div>
    <h4>PACIENTE</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            DPI :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dpi)
        </dd>
        <dt>
            Nombre :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            Apellido :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.apellido)
        </dd>

        <dt>
            Dirección :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.direccion)
        </dd>

        <dt>
            Fecha de Nacimiento :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fechaNacimiento)
        </dd>

        <dt>
            Fecha de Registro :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fechaRegistro)
        </dd>
        <dt>
            Cantidad de Citas :
        </dt>

        <dd>
            @ViewBag.totalCitas
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Regresar a lista de Reportes", "Index")
</p>
